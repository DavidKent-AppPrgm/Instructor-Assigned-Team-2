<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.51" fx:controller="application.controller.KioskController">
    <!-- TODO Add Nodes -->
    <left>
        <AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: lavender;">
            <children>
                <Label layoutX="24.0" layoutY="27.0" prefHeight="50.0" prefWidth="200.0" text="Kiosk">
                    <font>
                        <Font size="48.0"/>
                    </font>
                </Label>
                <ToolBar layoutX="430.0" layoutY="14.0" prefWidth="348.0">
                    <items>
                        <Button fx:id="buttonKioskSearch" mnemonicParsing="false" onAction="#buttonKioskSearchClicked"
                                text="Search"/>
                        <TextField id="textFieldSearch" fx:id="textFieldKioskSearch" prefWidth="264.0"
                                   promptText="search"/>
                    </items>
                </ToolBar>
                <Button fx:id="buttonNextPage" alignment="CENTER_LEFT" layoutX="550.0" layoutY="91.0" minWidth="68.0"
                        mnemonicParsing="false" onAction="#buttonNextPageClicked" prefHeight="20.0" prefWidth="90.0"
                        text="Next Page">
                    <font>
                        <Font size="14.0" fx:id="x1"/>
                    </font>
                </Button>
                <Button id="buttonNextPage" fx:id="buttonPreviousPage" alignment="CENTER_LEFT" layoutX="24.0"
                        layoutY="91.0" minWidth="68.0" mnemonicParsing="false" onAction="#buttonPreviousPageClicked"
                        prefHeight="25.0" prefWidth="90.0" text="Previous Page" visible="false">
                    <font>
                        <Font size="11.0"/>
                    </font>
                </Button>
                <TextArea fx:id="textAreaKiosk0" layoutX="24.0" layoutY="159.0" minHeight="-Infinity" prefHeight="100.0"
                          prefWidth="200.0" wrapText="true"/>
                <TextArea fx:id="textAreaKiosk3" layoutX="24.0" layoutY="350.0" minHeight="-Infinity" prefHeight="100.0"
                          prefWidth="200.0" wrapText="true"/>
                <TextArea fx:id="textAreaKiosk1" layoutX="230.0" layoutY="159.0" minHeight="-Infinity"
                          prefHeight="100.0" prefWidth="200.0" wrapText="true"/>
                <TextArea fx:id="textAreaKiosk4" layoutX="230.0" layoutY="350.0" minHeight="-Infinity"
                          prefHeight="100.0" prefWidth="200.0" wrapText="true"/>
                <TextArea fx:id="textAreaKiosk2" layoutX="436.0" layoutY="159.0" minHeight="-Infinity"
                          prefHeight="100.0" prefWidth="200.0" wrapText="true"/>
                <TextArea fx:id="textAreaKiosk5" layoutX="436.0" layoutY="350.0" minHeight="-Infinity"
                          prefHeight="100.0" prefWidth="200.0" wrapText="true"/>
                <Button fx:id="buttonKioskAdminPage" layoutX="190.0" layoutY="47.0" mnemonicParsing="false"
                        onAction="#buttonKioskAdminPageClicked" prefHeight="32.0" prefWidth="80.0" text="Admin"
                        visible="false"/>
                <Button id="buttonKioskAdminPage" fx:id="buttonKioskEmployeePage" layoutX="190.0" layoutY="15.0"
                        mnemonicParsing="false" onAction="#buttonKioskEmployeePageClicked" prefHeight="32.0"
                        prefWidth="80.0" text="Employee" visible="false"/>
                <Button id="buttonAdminLogout" fx:id="buttonKioskLogout" font="$x1" layoutX="270.0" layoutY="15.0"
                        mnemonicParsing="false" onAction="#buttonKioskLogoutClicked" prefHeight="32.0" prefWidth="80.0"
                        text="Logout"/>
                <TextArea fx:id="textAreaCart" layoutX="653.0" layoutY="123.0" prefHeight="382.0" prefWidth="140.0"
                          wrapText="true"/>
                <Label font="$x1" layoutX="683.0" layoutY="91.0" prefHeight="25.0" prefWidth="80.0" text="Check Out:"/>
                <Button fx:id="buttonAddToCart0" layoutX="83.0" layoutY="279.0" mnemonicParsing="false"
                        onAction="#buttonAddToCartClicked" text="Add to Cart"/>
                <Button fx:id="buttonAddToCart1" layoutX="294.0" layoutY="279.0" mnemonicParsing="false"
                        onAction="#buttonAddToCartClicked" text="Add to Cart"/>
                <Button fx:id="buttonAddToCart2" layoutX="500.0" layoutY="279.0" mnemonicParsing="false"
                        onAction="#buttonAddToCartClicked" text="Add to Cart"/>
                <Button fx:id="buttonAddToCart3" layoutX="83.0" layoutY="475.0" mnemonicParsing="false"
                        onAction="#buttonAddToCartClicked" text="Add to Cart"/>
                <Button fx:id="buttonAddToCart4" layoutX="294.0" layoutY="475.0" mnemonicParsing="false"
                        onAction="#buttonAddToCartClicked" text="Add to Cart"/>
                <Button fx:id="buttonAddToCart5" layoutX="500.0" layoutY="475.0" mnemonicParsing="false"
                        onAction="#buttonAddToCartClicked" text="Add to Cart"/>
                <Button fx:id="buttonClearCart" font="$x1" layoutX="681.0" layoutY="516.0" mnemonicParsing="false"
                        onAction="#buttonClearCartClicked" prefHeight="32.0" prefWidth="82.0" text="Clear Cart"
                        visible="false"/>
                <Button fx:id="buttonBuy" font="$x1" layoutX="681.0" layoutY="564.0" mnemonicParsing="false"
                        onAction="#buttonBuyClicked" prefHeight="32.0" prefWidth="85.0" text="Buy" visible="false"/>
                <Button id="buttonAdminLogout" font="$x1" layoutX="350.0" layoutY="15.0" mnemonicParsing="false"
                        onAction="#buttonKioskHomeClicked" prefHeight="32.0" prefWidth="80.0" text="Home"/>
            </children>
        </AnchorPane>
    </left>
</BorderPane>
